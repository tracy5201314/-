page{
  background: #f7f7f7;
}
.mt-size{
  margin-top: 30rpx;
}
.contacts{
  padding: 0 30rpx 30rpx 30rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.contacts-mbox{}
.contacts-list{}
.contacts-item{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
  padding: 30rpx 84rpx 30rpx 22rpx;
  background: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.contacts-item-icon{
  position: absolute;
  top: 0;
  right: 0;
  z-index: 2;
  display: block;
  width: 84rpx;
  height: 70rpx;
}
.contacts-item-tximg{
  width: 122rpx;
  height: 122rpx;
  border: 6rpx solid #fff;
  border-radius: 50%;
  box-sizing: border-box;
}
.contacts-item-tximg image{
  display: block;
  width: 100%;
  height: 100%;
}
.contacts-item-textbox{
  flex: 1;
  margin-left: 25rpx;
  overflow: hidden;
}
.contacts-item-info1{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.contacts-item-info1 text{
  font-weight: 600;
  font-size: 32rpx;
  color: #333;
}
.contacts-item-info1-uname{}
.contacts-item-info1-uage{
  margin-left: 20rpx;
}
.contacts-item-info1-gender{
  margin-left: 20rpx;
}
.contacts-item-info1-icon{
  width: 82rpx;
  height: 32rpx;
  margin-left: 20rpx;
  line-height: 32rpx;
  font-weight: normal;
  text-align: center;
  font-size: 24rpx;
  color: #ff9e2b;
  border-radius: 17rpx;
  border: 1px solid #ff9e2b;
}
.contacts-item-info2{
  margin-top: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.contacts-item-info2-title{
  font-size: 28rpx;
  color: #999;
}
.contacts-item-info2-text{
  font-size: 28rpx;
  color: #333;
}
.contacts-item-info3{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.contacts-btnbox{
  margin-top: 80rpx;
}
.contacts-btn{
  display: block;
  margin-top: 110rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 45rpx;
  background: -webkit-linear-gradient(left,#52a3ff,#1f7ef7);
  box-shadow: 0rpx 0rpx 10rpx rgba(185,215,253,1);
}
.contacts-btn text{
  font-weight: 600;
  font-size: 36rpx;
  color: #fff;
}