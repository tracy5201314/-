page{
  height: 100%;
  background-color: #fff;
}
.success{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100%;
}
.success image{
  width: 160rpx;
  height: 176rpx;
}
.success .text{
  margin: 62rpx 0 30rpx;
  font-size: 42rpx;
	font-weight: 700;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 0rpx;
	color: #333333;
}
.success .tips{
	font-size: 24rpx;
	font-weight: 400;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 0rpx;
	color: #999999;
  text-align: center;
}

/*按钮*/
.btn{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 185rpx;
}
.btn button{
  margin: 0;
  padding: 0;
  width: 288rpx;
	height: 82rpx;
  border: none;
  border-radius: 41rpx;
  background-image: linear-gradient(-90deg, #3a6fff 0%, #52a3ff 100%);
	box-shadow: 0rpx 3rpx 24rpx 0rpx rgba(59, 114, 255, 0.36);
  font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 82rpx;
	letter-spacing: 0rpx;
	color: #ffffff;
}
.btn button::after{
  border: none;
}
.btn button:first-child{
  margin-right: 28rpx;
  background-color: #ffffff;
  background-image: none;
	box-shadow: 0rpx 3rpx 16rpx 0rpx rgba(197, 197, 197, 0.2);
  color: #333333;
}